{
    "name": "clesson-de/traitor",
    "description": "A simple extension for Silverstripe CMS, which logs the author of each record when it is created or updated.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/clesson-de/traitor",
    "keywords": [
        "silverstripe",
        "extension",
        "creator",
        "editor",
        "author"
    ],
    "authors": [
        {
            "name": "Kai Feldmaier",
            "email": "kai.feldmaier@clesson.de",
            "homepage": "https://clesson.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "support": {
        "issues": "https://github.com/clesson-de/traitor/issues"
    },
    "autoload": {
        "psr-4": {
            "Clesson\\Traitor\\": "src/",
            "Clesson\\Traitor\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
